{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1280px"
    },
    "typography": {
      "fontSize": true,
      "fontStyle": true,
      "fontWeight": true,
      "lineHeight": true,
      "textDecoration": true,
      "letterSpacing": true,
      "textTransform": true,
      "dropCap": true
    },
    "color": {
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "link": false
    },
    "border": {
      "color": true,
      "customColor": true,
      "customRadius": true,
      "customStyle": true,
      "customWidth": true
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "2.5rem"
    }
  }
}